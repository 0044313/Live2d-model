          },
          {
            "name": "idle_times",
            "type": 2,
            "code": "assign 0"
          }
        ]
      },
      {
        "file": "mtn/Live2D_Chino015.mtn",
        "text": "もう…しょうがないアーサーさんですね。…少しだけなら触ってもいいですよ。",
        "sound": "sound/150.wav",
