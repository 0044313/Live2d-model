          }
        ]
      },
      {
        "file": "mtn/Live2D_elfin_0020.mtn",
        "text": "えへへ、気持ちいいからもっと撫でてもいいよ！",
        "sound": "sound/200.wav",
        "var_floats": [
          {
            "name": "idle_times",
